<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="PozNotificationMail" table="poz_notification_mail">
    <id name="notifmailId" type="integer" column="notifmail_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="notifmailName" type="string" column="notifmail_name" length="255" nullable="false"/>
    <field name="notifmailCat" type="integer" column="notifmail_cat" nullable="false"/>
  </entity>
</doctrine-mapping>
