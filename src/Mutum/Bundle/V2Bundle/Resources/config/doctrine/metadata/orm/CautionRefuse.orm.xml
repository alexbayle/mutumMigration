<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="CautionRefuse" table="caution_refuse">
    <id name="caurId" type="integer" column="caur_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="caurReported" type="boolean" column="caur_reported" nullable="false"/>
    <field name="caurReportedMessage" type="text" column="caur_reported_message" nullable="false"/>
    <field name="caurMessage" type="text" column="caur_message" nullable="false"/>
  </entity>
</doctrine-mapping>
