<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="RequestStatus" table="request_status">
    <id name="reqsId" type="integer" column="reqs_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="reqsName" type="string" column="reqs_name" length="255" nullable="false"/>
    <field name="reqsBorrowerInfo" type="string" column="reqs_borrower_info" length="255" nullable="false"/>
    <field name="reqsLenderInfo" type="string" column="reqs_lender_info" length="255" nullable="false"/>
  </entity>
</doctrine-mapping>
