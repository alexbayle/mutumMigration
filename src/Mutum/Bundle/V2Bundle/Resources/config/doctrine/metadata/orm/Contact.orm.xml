<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Contact" table="contact">
    <indexes>
      <index name="cont_user_id_b" columns="cont_user_id_b"/>
    </indexes>
    <id name="contUserIdA" type="integer" column="cont_user_id_a"/>
    <id name="contUserIdB" type="integer" column="cont_user_id_b"/>
    <field name="contDateCreation" type="datetime" column="cont_date_creation" nullable="false"/>
    <field name="contDateAccepted" type="datetime" column="cont_date_accepted" nullable="true"/>
    <field name="contStatus" type="string" column="cont_status" length="1" nullable="false"/>
  </entity>
</doctrine-mapping>
